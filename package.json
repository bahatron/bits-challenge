{
    "name": "bits-challenge",
    "version": "1.0.0",
    "description": "Coding challenge for Bits",
    "scripts": {
        "test": "jest --detectOpenHandles --forceExit",
        "build": "tsc -p tsconfig.build.json",
        "build:clean": "rm -rf dist && npm run build",
        "build:watch": "npm run build -- --watch",
        "dev": "ts-node-dev --transpile-only src/index.ts",
        "docker": "docker compose up --build",
        "docker:clean": "docker compose down --remove-orphans --volumes && npm run docker -- --force-recreate --renew-anon-volumes",
        "cdk:local": "rm -rf cdk.out && cdklocal bootstrap && cdklocal deploy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bahatron/bits-challenge.git"
    },
    "author": "simonpiscitelli@gmail.com",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/bahatron/bits-challenge/issues"
    },
    "homepage": "https://github.com/bahatron/bits-challenge#readme",
    "dependencies": {
        "@aws-cdk/aws-apigateway": "^1.38.0",
        "@aws-cdk/aws-dynamodb": "^1.32.2",
        "@aws-cdk/aws-iam": "^1.204.0",
        "@aws-cdk/aws-lambda": "^1.19.0",
        "@aws-sdk/client-dynamodb": "^3.616.0",
        "@aws-sdk/lib-dynamodb": "^3.616.0",
        "@sinclair/typebox": "^0.32.34",
        "@types/aws-lambda": "^8.10.141",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.11",
        "@types/supertest": "^6.0.2",
        "ajv": "^8.17.1",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^3.0.1",
        "aws-cdk": "^2.149.0",
        "aws-cdk-lib": "^2.149.0",
        "express": "^4.19.2",
        "express-async-errors": "^3.1.1",
        "faker": "^5.5.3",
        "jest": "^29.7.0",
        "pino": "^9.3.1",
        "redoc": "^2.1.5",
        "redoc-express": "^2.1.0",
        "serverless-http": "^3.2.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.3",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.3"
    }
}
